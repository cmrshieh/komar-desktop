xsetroot -cursor_name left_ptr -solid '#000000'
setxkbmap pl
xmodmap ~/.Xmodmap
xset r rate 250 20
export LC_ALL=pl_PL
urxvt -name stats -e tail -n 1000 -f /var/log/messages &
urxvt -name stats -e ncmpcpp &
urxvt -name stats -e htop &
gkrellm -g 150x1000+1129+14 &
gajim &
xscreensaver &
exec /usr/bin/xmonad

